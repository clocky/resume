{
  "name": "json-resume",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "webhint": "npx hint -c .hintrc http://mark.mclaughlin.me.uk",
    "watch:sass": "sass  --no-source-map --watch src/sass:dist/css",
    "watch:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "build:sass": "sass  --no-source-map src/sass:dist/css",
    "build:eleventy": "ELEVENTY_ENV=prod eleventy",
    "postbuild": "lightningcss --minify --targets '> 0.25%, not IE 11' dist/css/style.css -o dist/css/style.css && gzipper compress --zopfli --brotli ./dist",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass build:eleventy"
  },
  "author": "clocky",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "dayjs": "^1.11.5",
    "lightningcss-cli": "^1.15.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.54.9"
  },
  "browserslist": [
    "> 0.25%",
    "not IE 11"
  ],
  "devDependencies": {
    "gzipper": "^7.1.0",
    "prettier": "^2.7.1"
  }
}